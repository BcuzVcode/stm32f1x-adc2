    <conditions>   	
		<!-- Device Conditions -->
		<condition id="N32G4FR">
		  <description>NSING N32G4FR Devices</description> 
		  <require Dvendor="NSING:185" Dname="N32*"/> 
		</condition>
		
		<!-- Device + CMSIS Conditions -->
        <condition id="N32G4FR CMSIS">            
		    <description>NSING N32G4FR Device and the ARM compiler</description>
		    <require condition="N32G4FR"/>
		    <require Tcompiler="ARMCC"/>
        </condition>

		<!-- N32G4FR STDPERIPHERALS MISC Conditions -->
        <condition id="N32G4FR STDPERIPHERALS">
            <description>NSING N32G4FR Standard Peripherals drivers</description>
            <require condition="N32G4FR CMSIS"/>
            <require Cclass="Device" Cgroup="N32G4FR_StdPeripherals" Csub="MISC"/>
        </condition>
		
		<!-- N32G4FR STDPERIPHERALS RCC Conditions -->
        <condition id="N32G4FR STDPERIPHERALS RCC">
            <description>NSING N32G4FR Standard Peripherals driver with RCC</description>
            <require condition="N32G4FR STDPERIPHERALS"/>
            <require Cclass="Device" Cgroup="N32G4FR_StdPeripherals" Csub="RCC"/>
        </condition>
		
		<condition id="N32G4FR Algorithm Common" >
            <description>NSING N32G4FR Series Algorithm Libraries Common Functions.</description>
            <require condition="N32G4FR CMSIS"/>
            <require Cclass ="Device" Cgroup="Algorithm Libs" Csub="Common"/>
        </condition>

    </conditions>

