        <condition id="Helium">
            <description>Support All processors with Helium</description>
            <accept  Dmve="MVE"/>
            <accept  Dmve="FP_MVE"/>
        </condition>
        
        <condition id="CMSIS-CORE">
            <description>Require CMSIS-CORE Support</description>
            <require Cclass="CMSIS" Cgroup="CORE"/>
        </condition>
        
        <condition id="CMSIS-DSP">
            <description>Require CMSIS-DSP Support</description>
            <require condition="CMSIS-CORE"/>
            <require Cclass="CMSIS" Cgroup="DSP" Cversion="1.8.0"/>
        </condition>
        
        <condition id="Arm-2D-CORE">
            <description>Require Arm-2D Core Service</description>
            <require Cclass="Acceleration" Cgroup="Arm-2D" Csub="Core"/>
        </condition>

        <condition id="Arm-2D-Controls">
            <description>Require Arm-2D Core Service</description>
            <require condition="Arm-2D-CORE"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D" Csub="Alpha-Blending"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D" Csub="Transform"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D" Csub="Filter"/>
        </condition>
        
        <condition id="Arm-2D-PFB">
            <description>Require Arm-2D Core Service</description>
            <require condition="Arm-2D-CORE"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="PFB"/>
        </condition>

        <condition id="Arm-2D-Printf">
            <description>Require Arm-2D Core Service</description>
            <require condition="Arm-2D-CORE"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D" Csub="Alpha-Blending"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D" Csub="Transform"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="PFB"/>
        </condition>
        
        <condition id="CMSIS-RTOS2">
            <description>Require CMSIS-RTOS2</description>
            <require Cclass="CMSIS" Cgroup="RTOS2"/>
        </condition>
        
        <condition id="PFB CMSIS-RTOS2">
            <description>Require CMSIS-RTOS2</description>
            <require condition="CMSIS-RTOS2"/>
            <require condition="Arm-2D-PFB"/>
        </condition>
        
        <!--
        <condition id="Arm-2D-DSP">
            <description>Require Arm-2D Core Service</description>
            <require condition="Arm-2D-CORE"/>
            <require condition="CMSIS-DSP"/>
        </condition>
        -->
        <condition id="Arm-2D-Helper">
            <description>Require Arm-2D Helper Services</description>
            <require condition="Arm-2D-CORE"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="PFB" Cversion="2.0.0" />
            <require Cclass="Acceleration" Cgroup="Arm-2D Extras" Csub="LCD ASCII Printf"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D Extras" Csub="Controls"/>
        </condition>

        <condition id="Arm-2D-Displayer-Adapter">
            <description>The dependency of the Display Adapter</description>
            <require condition="Arm-2D-CORE"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D" Csub="Alpha-Blending"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="PFB" Cversion="2.0.0" />
            <require Cclass="Acceleration" Cgroup="Arm-2D Extras" Csub="LCD ASCII Printf"/>
        </condition>

        <condition id="Arm-2D-Scene">
            <description>The dependency of the scene</description>
            <require condition="Arm-2D-Displayer-Adapter"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="Display Adapter" />
        </condition>

        <condition id="Arm-2D-Demos">
            <description>The dependency of the scene</description>
            <require condition="Arm-2D-Helper"/>
            <require Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="Display Adapter" />
        </condition>
        
        <condition id="Arm-2D-Benchmark-Generic">
            <description>The Dependency Requirement for running arm-2d generic bemchmark</description>
            <require condition="Arm-2D-Helper"/>
            <deny Cclass="Acceleration" Cgroup="Arm-2D Extras" Csub="Benchmark-WatchPanel" />
        </condition>
        
        <condition id="Arm-2D-Benchmark-WatchPanel">
            <description>The Dependency Requirement for running arm-2d watch-panel bemchmark</description>
            <require condition="Arm-2D-Helper"/>
            <deny Cclass="Acceleration" Cgroup="Arm-2D Extras" Csub="Benchmark-Generic" />
        </condition>

        <condition id="IO">
          <description>Deny selection of Compiler class when using CMSIS-Compiler</description>
          <deny Cclass="Compiler" Cgroup="I/O"/>
        </condition>

        <condition id="CORE">
          <description>CMSIS-Compiler CORE component</description>
          <require condition="IO"/>
          <require Cclass="CMSIS-Compiler" Cgroup="CORE"/>
        </condition>

        <condition id="STDOUT LCD Console">
          <description>Dependency for retarget printf to an LCD console</description>
          <require condition="CORE"/>
          <require condition="Arm-2D-Helper"/>
          <accept Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="RTOS Helper" Cvariant="CMSIS-RTOS2"/>
          <accept Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="RTOS Helper" Cvariant="RT-Thread"/>
          <accept Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="RTOS Helper" Cvariant="User Custom"/>
          <require Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="Display Adapter"/>
        </condition>

        <condition id="STDOUT Disp Adapter0">
          <description>Dependency for retarget printf to the display adapter 0</description>
          <require condition="CORE"/>
          <require Cclass="Acceleration" Cgroup="Arm-2D Helper" Csub="Display Adapter"/>
        </condition>

        
